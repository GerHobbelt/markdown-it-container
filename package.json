{
  "name": "@gerhobbelt/markdown-it-container",
  "version": "3.0.0-7",
  "description": "Plugin to create block-level custom containers for markdown-it markdown parser",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown"
  ],
  "repository": "GerHobbelt/markdown-it-container",
  "license": "MIT",
  "type": "module",
  "source": "index.js",
  "main": "dist/markdownItContainer.js",
  "engines": {
    "node": ">=12.20.0"
  },
  "scripts": {
    "build": "make",
    "test": "make test",
    "todo": "make todo",
    "clean": "make clean",
    "superclean": "make superclean",
    "doc": "make doc",
    "lint": "make lint",
    "lintfix": "make lintfix",
    "cov": "make report-coverage",
    "pub": "npm publish --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@gerhobbelt/markdown-it": "12.0.4-52",
    "@gerhobbelt/markdown-it-testgen": "0.1.6-17",
    "@gerhobbelt/prepend-header": "1.1.0-7",
    "argparse": "2.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.19.0",
    "microbundle": "0.13.0",
    "mocha": "8.2.1",
    "nyc": "15.1.0"
  },
  "reporter": [
    "lcov",
    "text",
    "html",
    "text-summary"
